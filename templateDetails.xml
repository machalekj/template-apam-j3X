    <position>apam-nad-hlavnim-obsahem</position>
    <position>apam-pod-hlavnim-obsahem</position>
  </positions>
  <languages folder="language">
    <language tag="en-GB">en-GB/en-GB.tpl_apam.ini</language>
    <language tag="en-GB">en-GB/en-GB.tpl_apam.sys.ini</language>
    <language tag="cs-CZ">cs-CZ/cs-CZ.tpl_apam.ini</language>
    <language tag="en-GB">cs-CZ/cs-CZ.tpl_apam.sys.ini</language>
